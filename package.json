{
  "name": "fftcg-collection",
  "version": "0.5.0",
  "private": true,
  "author": "Micriber",
  "license": "ISC",
  "description": "Mobile app for organizing your Final Fantasy Trading Card Game.",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "android-release": "cd android && ./gradlew assembleRelease",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coverage:all": "jest --coverage --collectCoverageFrom=src/**/*.{js,jsx,ts,tsx}",
    "test:update": "jest -u",
    "lint": "eslint --max-warnings 0 --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix ./src",
    "clean": "rimraf android/app/build",
    "gradlew:clean": "cd android && gradlew clean",
    "ts:check": "tsc --noemit",
    "local:check-and-fix": "yarn run test:update && yarn run lint:fix && yarn run ts:check"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:micriber/fftcg-collection-app.git"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "@react-native-community/google-signin": "^4.0.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/admob": "^11.5.0",
    "@react-native-firebase/app": "^11.5.0",
    "@react-navigation/bottom-tabs": "^5.8.0",
    "@react-navigation/drawer": "5.12.x",
    "@react-navigation/material-bottom-tabs": "^5.3.13",
    "@react-navigation/native": "^5.9.x",
    "@react-navigation/stack": "^5.14.x",
    "appcenter": "4.1.0",
    "appcenter-analytics": "4.1.0",
    "appcenter-crashes": "4.1.0",
    "colortranslator": "^1.7.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-appearance": "^0.3.4",
    "react-native-config": "^1.3.3",
    "react-native-fast-image": "^8.3.2",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-image-colors": "^1.3.0",
    "react-native-modal-selector": "^2.0.2",
    "react-native-numeric-input": "^1.8.3",
    "react-native-paper": "^4.7.1",
    "react-native-reanimated": "^1.13.0",
    "react-native-safe-area-context": "^3.1.4",
    "react-native-screens": "^2.10.1",
    "react-native-super-grid": "^4.1.1",
    "react-native-svg": "^12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-vector-icons": "^7.0.0",
    "react-string-replace": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@testing-library/react-hooks": "^3.4.1",
    "@testing-library/react-native": "^7.0.2",
    "@types/jest": "^25.2.3",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.isempty": "^4.4.6",
    "@types/react-native": "^0.63.2",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jest/setup.js"
    ],
    "setupFilesAfterEnv": [
      "./jest/timeout.js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "<rootDir>/jest/mocks/fileMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
